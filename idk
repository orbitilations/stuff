local RunService = game:GetService("RunService")
local UIS = game:GetService("UserInputService")
local player = game:GetService("Players").LocalPlayer
local character = player.Character

a = Instance.new("Folder")
a.Name = "abatakum2"
a.Parent = game.Workspace

b = Instance.new("Part")
b.Name = "abatakum"
b.Parent = game.Workspace.abatakum2
b.CanCollide = false
b.BrickColor = BrickColor.new("Maroon")
b.Material = "ForceField"
b.Size = Vector3.new(1, 2, 2)

--// THE ANTI-AIM ITSELF
ANTIAIMZ = false
RunService.RenderStepped:Connect(function()
    UIS.InputBegan:Connect(function(input)
    	if input.KeyCode == Enum.KeyCode.F then
    	    if ANTIAIMZ == false then
    	        ANTIAIMZ = true
    	    else
    	        ANTIAIMZ = false
    	    end
    	end
    end)
end)

local anchorparts = game:GetService("Workspace").abatakum2

RunService.RenderStepped:Connect(function()
    if ANTIAIMZ == false then
        for i,v in pairs(anchorparts:GetDescendants()) do
            if v:IsA("Part") then
                v.CFrame = game:GetService("Workspace").Orbitilations.HumanoidRootPart.CFrame
            end
        end
    end
end)

RunService.RenderStepped:Connect(function()game:GetService("Workspace").abatakum2.abatakum.CFrame
    if ANTIAIMZ == true then
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = anchorparts.abatakum.CFrame *
        CFrame.new(math.random(1, 2) == 1 and 2 or -2, 0, 0)
    end
end)
